#!/home/aleksiks/prog/BD/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ddlgenerator==0.1.9','console_scripts','ddlgenerator'
__requires__ = 'ddlgenerator==0.1.9'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ddlgenerator==0.1.9', 'console_scripts', 'ddlgenerator')()
    )
